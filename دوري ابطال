# ---------- Helpers ----------
def safe_label(text, font_size=14, halign='left', valign='middle', markup=False, size_hint=(1, None), height=None):
    lbl = Label(text=text, font_size=font_size, markup=markup, size_hint=size_hint)
    if height:
        lbl.height = dp(height)
    def _update_text_size(instance, value):
        instance.text_size = (instance.width, None if instance.size[1] == 0 else instance.height)
    lbl.bind(size=_update_text_size)
    return lbl

# ---------- Styled widgets ----------
class ModernLabel(Label):
    def __init__(self, **kwargs):
        super().__init__(**kwargs)
        self.color = (1, 1, 1, 1)
        self.markup = kwargs.get('markup', False)
        def _update_text_size(instance, value):
            instance.text_size = (instance.width, None if instance.size[1] == 0 else instance.height)
        self.bind(size=_update_text_size)

class ModernButton(Button):
    def __init__(self, **kwargs):
        super().__init__(**kwargs)
        self.background_normal = ''
        if 'background_color' not in kwargs:
            self.background_color = (0, 0, 0, 0)
        self.color = (1, 1, 1, 1)
        self.font_size = kwargs.get('font_size', dp(14))
        self.markup = kwargs.get('markup', False)

class CardLayout(BoxLayout):
    def __init__(self, **kwargs):
        super().__init__(**kwargs)
        self.orientation = 'vertical'
        self.size_hint_y = None
        self.padding = dp(15)
        self.spacing = dp(8)
        with self.canvas.before:
            Color(0.15, 0.2, 0.3, 1)
            self.rect = RoundedRectangle(pos=self.pos, size=self.size, radius=[dp(12),])
        self.bind(pos=self.update_rect, size=self.update_rect)
    def update_rect(self, *args):
        self.rect.pos = self.pos
        self.rect.size = self.size

# ---------- Login Section (ŸÉŸÖÿß ŸÅŸä ŸÉŸàÿØŸÉ ÿßŸÑÿ£ÿµŸÑŸä) ----------
class LoginSection(BoxLayout):
    def __init__(self, on_login_success, **kwargs):
        super().__init__(**kwargs)
        self.orientation = 'vertical'
        self.spacing = dp(15)
        self.padding = dp(20)
        self.on_login_success = on_login_success
        self.create_ui()

    def create_ui(self):
        title = ModernLabel(
            text="[b]üîê Create Account / Login[/b]",
            font_size=dp(22),
            markup=True,
            halign='center',
            size_hint=(1, None),
            height=dp(40)
        )
        self.add_widget(title)
        # ÿ®ŸÇŸäÿ© ÿ≠ŸÇŸàŸÑ ÿßŸÑÿ™ÿ≥ÿ¨ŸäŸÑ ŸÉŸÖÿß ÿπŸÜÿØŸÉ‚Ä¶

        username_container = BoxLayout(orientation='vertical', size_hint=(1, None), height=dp(90), spacing=dp(5))
        username_label = ModernLabel(text="üë§ Username:", font_size=dp(16), size_hint=(1, None), height=dp(22))
        self.username_input = TextInput(
            multiline=False,
            font_size=dp(18),
            size_hint=(1, None),
            height=dp(44),
            background_color=(0.2, 0.25, 0.35, 1),
            foreground_color=(1, 1, 1, 1),
            padding=[dp(15), dp(12)],
            hint_text="Enter your username...",
            hint_text_color=(0.7, 0.7, 0.8, 0.7)
        )
        username_container.add_widget(username_label)
        username_container.add_widget(self.username_input)

        password_container = BoxLayout(orientation='vertical', size_hint=(1, None), height=dp(90), spacing=dp(5))
        password_label = ModernLabel(text="üîí Password:", font_size=dp(16), size_hint=(1, None), height=dp(22))
        self.password_input = TextInput(
            multiline=False,
            password=True,
            font_size=dp(18),
            size_hint=(1, None),
            height=dp(44),
            background_color=(0.2, 0.25, 0.35, 1),
            foreground_color=(1, 1, 1, 1),
            padding=[dp(15), dp(12)],
            hint_text="Enter your password...",
            hint_text_color=(0.7, 0.7, 0.8, 0.7)
        )
        password_container.add_widget(password_label)
        password_container.add_widget(self.password_input)

        strategy_container = BoxLayout(orientation='vertical', size_hint=(1, None), height=dp(140), spacing=dp(5))
        strategy_label = ModernLabel(text="üéØ Prediction Strategy:", font_size=dp(16), size_hint=(1, None), height=dp(22))
        self.strategy_input = TextInput(
            multiline=True,
            font_size=dp(16),
            size_hint=(1, 1),
            background_color=(0.2, 0.25, 0.35, 1),
            foreground_color=(1, 1, 1, 1),
            padding=[dp(15), dp(12)],
            hint_text="Describe your prediction strategy and approach...",
            hint_text_color=(0.7, 0.7, 0.8, 0.7)
        )
        strategy_container.add_widget(strategy_label)
        strategy_container.add_widget(self.strategy_input)

        buttons_container = BoxLayout(size_hint=(1, None), height=dp(44), spacing=dp(15))
        register_btn = ModernButton(text='‚úÖ Register New Account', font_size=dp(16), background_color=(0.2, 0.6, 0.2, 1))
        register_btn.bind(on_press=self.register_account)
        login_btn = ModernButton(text='üîê Login to Existing Account', font_size=dp(16), background_color=(0.2, 0.4, 0.8, 1))
        login_btn.bind(on_press=self.login_account)
        buttons_container.add_widget(register_btn)
        buttons_container.add_widget(login_btn)

        self.add_widget(username_container)
        self.add_widget(password_container)
        self.add_widget(strategy_container)
        self.add_widget(buttons_container)

    def register_account(self, instance):
        username = self.username_input.text.strip()
        password = self.password_input.text.strip()
        strategy = self.strategy_input.text.strip()
        if not username or not password:
            self.show_error("‚ùå Please fill in all fields!")
            return
        if len(username) < 3:
            self.show_error("‚ùå Username must be at least 3 characters!")
            return
        if len(password) < 4:
            self.show_error("‚ùå Password must be at least 4 characters!")
            return
        if not strategy:
            self.show_error("‚ùå Please describe your prediction strategy!")
            return
        user_key = f"{username}_{password}"
        users_data = self.load_users_data()
        if user_key in users_data:
            self.show_error("‚ùå Account already exists! Please login instead.")
            return
        users_data[user_key] = {
            'username': username,
            'password': password,
            'strategy': strategy,
            'join_date': datetime.now().strftime('%Y-%m-%d %H:%M'),
            'predictions_count': 0
        }
        self.save_users_data(users_data)
        self.on_login_success(users_data[user_key])
        self.show_success(f"üéâ Welcome {username}! Account created successfully.")

    def login_account(self, instance):
        username = self.username_input.text.strip()
        password = self.password_input.text.strip()
        if not username or not password:
            self.show_error("‚ùå Please enter username and password!")
            return
        users_data = self.load_users_data()
        user_key = f"{username}_{password}"
        if user_key not in users_data:
            self.show_error("‚ùå Invalid username or password!")
            return
        self.on_login_success(users_data[user_key])
        self.show_success(f"üëã Welcome back {username}!")

    def load_users_data(self):
        try:
            if os.path.exists("users_data.json"):
                with open("users_data.json", 'r') as f:
                    return json.load(f)
            return {}
        except:
            return {}

    def save_users_data(self, users_data):
        try:
            with open("users_data.json", 'w') as f:
                json.dump(users_data, f, indent=4)
        except:
            pass

    def show_error(self, message):
        popup = Popup(title='Error', content=ModernLabel(text=message, size_hint=(1, 1)), size_hint=(0.7, 0.3))
        popup.open()
        Clock.schedule_once(lambda dt: popup.dismiss(), 3)

    def show_success(self, message):
        popup = Popup(title='Success', content=ModernLabel(text=message, size_hint=(1, 1)), size_hint=(0.7, 0.3))
        popup.open()
        Clock.schedule_once(lambda dt: popup.dismiss(), 2)

# ---------- Modal ÿßŸÑÿ™ŸàŸÇÿπÿßÿ™ ÿßŸÑÿ™ŸÅÿµŸäŸÑŸäÿ© (ŸÉŸÖÿß ÿπŸÜÿØŸÉ) ----------
class DetailedPredictionModal(ModalView):
    def __init__(self, match, current_user, on_prediction_submit, **kwargs):
        super().__init__(**kwargs)
        self.match = match
        self.current_user = current_user
        self.on_prediction_submit = on_prediction_submit
        self.size_hint = (0.9, 0.8)
        self.background_color = (0.1, 0.15, 0.25, 0.95)
        self.create_ui()

    def create_ui(self):
        layout = BoxLayout(orientation='vertical', padding=dp(20), spacing=dp(15))
        title = ModernLabel(
            text=f"[b]üéØ Detailed Prediction: {self.match['team1']} vs {self.match['team2']}[/b]",
            font_size=dp(18), markup=True, halign='center'
        )
        layout.add_widget(title)
        user_info = ModernLabel(text=f"üë§ Predictor: {self.current_user['username']}", font_size=dp(14), color=(0.8, 0.9, 1, 1))
        layout.add_widget(user_info)

        # Score inputs
        score_layout = BoxLayout(size_hint=(1, None), height=dp(50), spacing=dp(20))
        score_layout.add_widget(ModernLabel(text="Final Score:", font_size=dp(16), size_hint=(0.4, None), height=dp(30)))
        self.team1_score = TextInput(text='0', multiline=False, input_filter='int', font_size=dp(18),
                                     halign='center', size_hint=(0.15, None), height=dp(40),
                                     background_color=(0.2, 0.25, 0.35, 1), foreground_color=(1, 1, 1, 1),
                                     padding=[dp(10), dp(10)])
        score_layout.add_widget(self.team1_score)
        score_layout.add_widget(ModernLabel(text=":", font_size=dp(20), size_hint=(0.05, None), height=dp(40)))
        self.team2_score = TextInput(text='0', multiline=False, input_filter='int', font_size=dp(18),
                                     halign='center', size_hint=(0.15, None), height=dp(40),
                                     background_color=(0.2, 0.25, 0.35, 1), foreground_color=(1, 1, 1, 1),
                                     padding=[dp(10), dp(10)])
        score_layout.add_widget(self.team2_score)
        layout.add_widget(score_layout)

        winner_layout = BoxLayout(size_hint=(1, None), height=dp(40), spacing=dp(10))
        winner_layout.add_widget(ModernLabel(text="üèÜ Winner:", font_size=dp(14), size_hint=(0.2, None), height=dp(30)))
        self.winner_input = TextInput(text=self.match['team1'], multiline=False,
                                      font_size=dp(14), size_hint=(0.6, None), height=dp(40),
                                      background_color=(0.2, 0.25, 0.35, 1), foreground_color=(1, 1, 1, 1),
                                      padding=[dp(10), dp(10)])
        winner_layout.add_widget(self.winner_input)
        layout.add_widget(winner_layout)

        scorers_layout = BoxLayout(orientation='vertical', spacing=dp(5), size_hint=(1, None), height=dp(110))
        scorers_layout.add_widget(ModernLabel(text="‚öΩ Goal Scorers:", font_size=dp(14)))
        self.scorers_input = TextInput(multiline=True, font_size=dp(13), size_hint=(1, 1),
                                       background_color=(0.2, 0.25, 0.35, 1), foreground_color=(1, 1, 1, 1),
                                       padding=[dp(10), dp(10)], hint_text="List goal scorers...")
        scorers_layout.add_widget(self.scorers_input)
        layout.add_widget(scorers_layout)

        analysis_layout = BoxLayout(orientation='vertical', spacing=dp(5), size_hint=(1, None), height=dp(110))
        analysis_layout.add_widget(ModernLabel(text="üìä Match Analysis:", font_size=dp(14)))
        self.analysis_input = TextInput(multiline=True, font_size=dp(13), size_hint=(1, 1),
                                        background_color=(0.2, 0.25, 0.35, 1), foreground_color=(1, 1, 1, 1),
                                        padding=[dp(10), dp(10)], hint_text="Your analysis and reasoning...")
        analysis_layout.add_widget(self.analysis_input)
        layout.add_widget(analysis_layout)

        buttons_layout = BoxLayout(size_hint=(1, None), height=dp(44), spacing=dp(10))
        cancel_btn = ModernButton(text='‚ùå Cancel', on_press=lambda x: self.dismiss())
        submit_btn = ModernButton(text='‚úÖ Submit Prediction')
        submit_btn.bind(on_press=self.submit_prediction)
        buttons_layout.add_widget(cancel_btn)
        buttons_layout.add_widget(submit_btn)
        layout.add_widget(buttons_layout)

        self.add_widget(layout)

    def submit_prediction(self, instance):
        try:
            prediction_data = {
                'score': f"{self.team1_score.text}-{self.team2_score.text}",
                'winner': self.winner_input.text,
                'scorers': self.scorers_input.text,
                'analysis': self.analysis_input.text,
                'username': self.current_user['username'],
                'strategy': self.current_user['strategy'],
                'timestamp': datetime.now().strftime('%Y-%m-%d %H:%M')
            }
            self.on_prediction_submit(self.match, prediction_data)
            self.dismiss()
        except Exception as ex:
            popup = Popup(title='Error', content=ModernLabel(text="‚ùå Please check your inputs!"), size_hint=(0.6, 0.3))
            popup.open()
            Clock.schedule_once(lambda dt: popup.dismiss(), 2)

# ---------- ÿßŸÑÿ™ÿ∑ÿ®ŸäŸÇ ÿßŸÑÿ±ÿ¶Ÿäÿ≥Ÿä ----------
class ChampionsLeagueApp(App):
    def build(self):
        self.title = "UEFA Champions League üèÜ"
        self.data_manager = DataManager()
        self.user_predictions = {}
        self.current_user = None

        self.main_layout = BoxLayout(orientation='vertical', spacing=dp(5))
        self.create_header()
        self.create_tabs()
        Clock.schedule_once(lambda dt: self.load_data(), 1)
        return self.main_layout

    def create_header(self):
        header = BoxLayout(size_hint=(1, None), height=dp(60), padding=dp(10))
        with header.canvas.before:
            Color(0.1, 0.15, 0.25, 1)
            Rectangle(pos=header.pos, size=header.size)
        title = Label(text='üèÜ UEFA Champions League', font_size=dp(20), color=(1,1,1,1))
        self.user_btn = ModernButton(text='üë§ Login', size_hint=(0.25, 1), on_press=lambda x: self.show_login_tab())
        header.add_widget(title)
        header.add_widget(self.user_btn)
        self.main_layout.add_widget(header)

    def create_tabs(self):
        self.tabs = TabbedPanel(size_hint=(1, 1), do_default_tab=False)
        # Matches tab
        matches_tab = TabbedPanelItem(text='‚öΩ Matches')
        self.matches_content = ScrollView()
        self.matches_layout = GridLayout(cols=1, spacing=dp(10), size_hint_y=None, padding=dp(10))
        self.matches_layout.bind(minimum_height=self.matches_layout.setter('height'))
        self.matches_content.add_widget(self.matches_layout)
        matches_tab.add_widget(self.matches_content)
        # Predictions tab
        predictions_tab = TabbedPanelItem(text='ü§î Predictions')
        self.predictions_content = ScrollView()
        self.predictions_layout = GridLayout(cols=1, spacing=dp(10), size_hint_y=None, padding=dp(10))
        self.predictions_layout.bind(minimum_height=self.predictions_layout.setter('height'))
        self.predictions_content.add_widget(self.predictions_layout)
        predictions_tab.add_widget(self.predictions_content)

        self.tabs.add_widget(matches_tab)
        self.tabs.add_widget(predictions_tab)
        self.main_layout.add_widget(self.tabs)

    def load_data(self):
        threading.Thread(target=self._load_data_thread, daemon=True).start()

    def _load_data_thread(self):
        matches = self.data_manager.get_live_matches()
        Clock.schedule_once(lambda dt: self.update_matches(matches), 0)

    def update_matches(self, matches):
        self.matches_layout.clear_widgets()
        for m in matches:
            self.matches_layout.add_widget(self.create_match_card(m))

    def create_match_card(self, match):
        card = CardLayout(size_hint_y=None, height=dp(120))
        info = BoxLayout(orientation='horizontal', spacing=dp(10))

        # ÿ¥ÿπÿßÿ±ÿßÿ™ ÿßŸÑŸÅÿ±ŸÇ
        def make_logo(team):
            url = team_logos.get(team)
            if url:
                return AsyncImage(source=url, size_hint=(None, None), size=(dp(50), dp(50)))
            else:
                # fallback ÿ•ŸÑŸâ ÿ•ŸäŸÖŸàÿ¨Ÿä
                return Label(text=team[0], font_size=dp(32), color=(1,1,1,1))

        logo1 = make_logo(match['team1'])
        logo2 = make_logo(match['team2'])

        team1_box = BoxLayout(orientation='vertical', size_hint=(0.4, 1))
        team1_box.add_widget(logo1)
        team1_box.add_widget(Label(text=match['team1'], font_size=dp(14), color=(1,1,1,1)))

        team2_box = BoxLayout(orientation='vertical', size_hint=(0.4, 1))
        team2_box.add_widget(logo2)
        team2_box.add_widget(Label(text=match['team2'], font_size=dp(14), color=(1,1,1,1)))

        center = BoxLayout(orientation='vertical', size_hint=(0.2, 1))
        center.add_widget(Label(text=match.get('score', ''), font_size=dp(16), color=(1,1,1,1)))
        center.add_widget(Label(text=match.get('date', ''), font_size=dp(12), color=(0.8,0.8,0.8,1)))

        info.add_widget(team1_box)
        info.add_widget(center)
        info.add_widget(team2_box)

        card.add_widget(info)

        return card

    def show_login_tab(self):
        self.tabs.switch_to(self.tabs.tab_list[1])
        self.predictions_layout.clear_widgets()
        self.predictions_layout.add_widget(LoginSection(self.on_login_success))

    def on_login_success(self, user_data):
        self.current_user = user_data
        self.user_btn.text = f"üë§ {user_data['username']}"
        # ÿ™ÿ≠ÿØŸäÿ´ ÿ™ŸàŸÇÿπÿßÿ™ ÿ•ÿ∞ÿß ÿπŸÜÿØŸÉ ŸÑÿßÿ≠ŸÇŸãÿß

